	 name:          "title";
	 type:          TEXT;
	 effect:        SOFT_SHADOW;
	 mouse_events:  0;
	 description {
	    state:    "default" 0.0;
	    fixed:    0 1;
	    align:    0.5 0.0;
	    rel1 {
	       relative: 0.0  0.0;
	       offset:   6    6;
	    }
	    rel2 {
	       relative: 1.0  0.0;
	       offset:   -7   6;
	    }
	    color: 255 255 255 255;
	    color3: 0 0 0 32;
	    text {
	       text:     "Enlightenment";
	       font:     "Edje-Vera-Bold";
	       size:     16;
	       min:      1 1;
	       align:    0.5 0.5;
               text_class: "about_title";	       
	    }
	 }
