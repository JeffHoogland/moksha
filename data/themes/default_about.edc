	    state:      "default" 0.0;
	    min:        63 63;
	    max:        63 63;
	    align:      0.5 0.5;
	    fixed:      1 1;
	    rel1 {
	       relative: 0.5  0.5;
	       offset:   0    0;
	       to_y:     "about";
	    }
	    rel2 {
