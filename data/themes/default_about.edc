   programs {
      program {
	 name:   "button_click";
	 signal: "mouse,down,1";
	 source: "button";
	 action: STATE_SET "clicked" 0.0;
	 target: "button";
      }
      program {
	 name:   "button_unclick";
	 signal: "mouse,up,1";
	 source: "button";
	 action: STATE_SET "default" 0.0;
	 target: "button";
      }
      program {
	 name:   "button_unclick2";
	 signal: "mouse,clicked,1";
	 source: "button";
	 action: SIGNAL_EMIT "close" "";
      }
      
      program {      
	 name:       "anim_sky1a";
	 signal:     "show";
	 source:     "";
	 action:     STATE_SET "drift" 0.0;
	 transition: LINEAR 20.0;
	 target:     "sky1";
	 after:      "anim_sky1b";
      }
      program {
	 name:       "anim_sky1b";
	 action:     STATE_SET "default" 0.0;
	 target:     "sky1";
	 after:      "anim_sky1a";
      }

      program {      
	 name:       "anim_sky4a";
	 signal:     "show";
	 source:     "";
