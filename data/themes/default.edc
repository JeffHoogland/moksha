		  normal: "vgrad_dark.png";
	       }
	       fill {
		  smooth: 0;
		  size {
		     relative: 0 1.0;
		     offset:   36 0;
		  }
	       }
	    }
	 }
	 part {
	    name: "grill";
	    description {
	       state: "default" 0.0;
	       image {
		  normal: "grill_dark_tiny_pattern.png";
	       }
	       fill {
