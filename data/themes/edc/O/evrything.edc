            }
         }
         part {
            name: "list";
            source: "e/modules/everything/list";
            type: GROUP;
            description {
               state: "default" 0.0;
               visible: 1;
               min: 300 155;
               rel1 {
                  to_y: "frame";
                  offset: -1 -3;
                  relative: 0.0 1.0;
              }
              rel2 {
                 offset: 0 -1;
               }
            }
         }
         part {
            name: "bar_base";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               rel1 {
                  to: "frame";
               }
               rel2 {
                  to: "frame";
               }
            }
         }
         part {
            name: "bar_bg";
