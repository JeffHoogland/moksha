               }
               rel2 {
                  relative: 0.0 1.0;
                  offset: 0 -1;
               }
            }
         }
         part {
            name: "action_selector";
            type: GROUP;
            source: "e/modules/everything/selector_item";
            clip_to: "selector_frame";
            description {
               state: "default" 0.0;
               /* visible: 0; */
               rel1 {
                  relative: 0.5 0.0;
                  offset: 0 2;
                  to: "selector_frame";
               }
               rel2 {
                  relative: 1.0 1.0;
                  offset: -1 -1;
                  to: "selector_frame";
               }
            }
            description {
               state: "wide" 0;
               inherit: "default" 0.0;
               rel1 {
                  relative: 0.0 0.0;
                  offset: 0 2;
               }
               rel2 {
                  relative: 0.5 1.0;
                  offset: -1 -1;
               }
            }
         }
         part {
            name: "object_selector";
            type: GROUP;
            source: "e/modules/everything/selector_item";
            clip_to: "selector_frame";
            description {
               state: "default" 0.0;
               visible: 0;
               rel1 {
                  relative: 1.0 0.0;
                  offset: 0 2;
                  to: "selector_frame";
               }
               rel2 {
                  relative: 1.5 1.0;
                  offset: -1 -1;
                  to: "selector_frame";
               }
            }
            description {
               state: "wide" 0;
               inherit: "default" 0.0;
               visible: 1;
               rel1 {
                  relative: 0.5 0.0;
                  offset: 0 2;
               }
               rel2 {
                  relative: 1.0 1.0;
                  offset: -1 -1;
               }
            }
         }

         part {
            name: "bar_over2";
            clip_to: "bar_base";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               rel1.to: "bar_bg";
               rel1.offset: -2 -2;
               rel2.to: "bar_bg";
               rel2.offset: 1 2;
               image {
                  normal: "O/bt_dis_shine.png";
                  border: 4 4 4 4;
               }
            }
         }
         part {
            name: "bar_over1";
            clip_to: "bar_base";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               rel1.to: "bar_bg";
               rel1.offset: -2 -2;
               rel2.to: "bar_bg";
               rel2.offset: 1 2;
               rel2.relative: 1.0 0.45;
               image {
                  normal: "O/bt_dis_hilight.png";
