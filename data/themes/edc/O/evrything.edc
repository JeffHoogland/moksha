         }
      }
   }
   group {
      name: "e/modules/everything/selector_item";
      parts {
         part {
            name: "clip";
            type: RECT;
            mouse_events: 0;
            description {
               state: "default" 0.0;
            }
         }
         part {
            name: "bg";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               color: 200 200 200 20;
               image {
                  normal: "O/toolbar_sel.png";
                  border: 3 3 2 2;
               }
               fill.smooth: 1;
               rel1.offset: 0 -1;
               rel2.offset: 2 -1;
            }
            description {
               state: "selected" 0.0;
               inherit: "default" 0.0;
               color: 200 200 200 255;
            }
         }
         part {
            name: "inner_clip";
