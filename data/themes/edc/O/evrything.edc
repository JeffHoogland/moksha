               state: "default" 0.0;
               /* min: 32 50;
                * max: 9999 50; */
               fixed: 1 1;
               rel1 {
                  offset: -2 -1;
                  to: "bar_base";
               }
               rel2 {
                  offset: 1 0;
                  to: "bar_base";
               }
               image {
                  normal: "O/bt_dis_base.png";
                  border: 4 4 4 4;
               }
            }
         }
         part {
            name: "frame";
            mouse_events: 0;;
            description {
               state: "default" 0.0;
               rel1.to: "win";
               rel2.to: "win";
               image {
                  normal: "O/everything_box.png";
                  border: 4 4 4 4;
                  middle: 0;
               }
               fill.smooth : 1;
              color: 215 215 215 255;
            }
         }
         part {
            name: "selector_frame";
            type: RECT;
            description {
               state: "default" 0.0;
               color: 255 255 255 255;
               rel1 {
                  to: "frame";
               }
               rel2 {
                  offset: -2 -1;
                  to: "frame";
               }
            }
         }
         part {
            name: "subject_selector";
