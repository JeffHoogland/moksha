               state: "default" 0.0;
               min: 50 44;
               max: 50 44;
               aspect: 1.0 1.0;
               align: 0.0 0.5;
               aspect_preference: VERTICAL;
               rel1 {
                  offset: 5   5;
               }
               rel2 {
                  offset: -1  -8;
               }
            }
         }
         part {
            name: "e.swallow.thumb";
            type: SWALLOW;
            clip_to: "icon_clip";
            mouse_events: 0;
            description {
               state: "default" 0.0;
               min: 50 44;
               max: 50 44;
               aspect: 1.0 1.0;
               align: 0.0 0.5;
               aspect_preference: VERTICAL;
               rel1 {
                  offset: 5   5;
               }
               rel2 {
                  offset: -1  -8;
               }
            }
         }

         part {
            name: "e.text.label";
            type: TEXT;
            mouse_events: 0;
            clip_to: "clip";
            scale: 1;
            mouse_events: 0;
            description {
               state: "default" 0.0;
               align: 0.0 1.0;
               fixed: 0 1;
               rel1.to: "e.swallow.icons";
               rel1.relative: 1.0 0.0;
               rel2.to: "clip";
               rel1.offset: 2 3;
               rel2.offset: -6 -7;
               /* visible: 0; */
               color: 24 24 24 255;
               text {
                  font: "Sans:style=Bold";
                  size: 9;
                  min: 0 1;
                  align: 0.0 1.0;
                  text_class: "fileman_typebuf";
               }
            }
            description {
               state: "selected" 0.0;
               inherit: "default" 0.0;
               color: 244 244 244 255;
               /* visible: 1; */
            }
         }
         part {
            name: "e.text.plugin";
            type: TEXT;
